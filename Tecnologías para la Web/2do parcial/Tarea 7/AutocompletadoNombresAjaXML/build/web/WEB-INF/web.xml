<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>AutocompletadoServlet</servlet-name>
        <servlet-class>AutocompletadoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServicioAutocompletado</servlet-name>
        <servlet-class>ServicioAutocompletado</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServicioComparador</servlet-name>
        <servlet-class>ServicioComparador</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AutocompletadoServlet</servlet-name>
        <url-pattern>/AutocompletadoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServicioAutocompletado</servlet-name>
        <url-pattern>/ServicioAutocompletado</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServicioComparador</servlet-name>
        <url-pattern>/ServicioComparador</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
